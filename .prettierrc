{
  "arrowParens": "always",
  "semi": false,
  "tabWidth": 2,
  "printWidth": 80,
  "singleQuote": false,
  "jsxSingleQuote": false,
  "trailingComma": "es5",
  "bracketSpacing": true,
  "endOfLine": "lf",
  "importOrder": [
    "^react$",
    "^vite$",
    "^react-router-dom$",
    "<THIRD_PARTY_MODULES>",
    "^@/hooks/(.*)$",
    "^@/utils/(.*)$",
    "^@/store(/*.*)$",
    "^@/assets/(.*)$",
    "^@/types/(.*)$",
    "^@/pages/(.*)$",
    "^@/layouts/(.*)$",
    "^@/components/(.*)$",
    "^[./]",
    "^[../]"
  ],
  "importOrderSeparation": false,
  "importOrderSortSpecifiers": true,
  "plugins": [
    "@trivago/prettier-plugin-sort-imports",
    "prettier-plugin-tailwindcss"
  ]
}
